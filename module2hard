def create_password(n):

    pairs = []


    for i in range(1, 21):
        for j in range(i, 21):

            if (i + j) % n == 0:
                # Если делится, добавляем пару в список
                pairs.append(str(i) + str(j))

    result = ''.join(pairs)
    return result
for n in range(3, 21):
    result = create_password(n)
    print(f"Пароль для числа {n}: {result}")
